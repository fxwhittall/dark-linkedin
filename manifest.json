{
  "name": "LinkedIn Dark Mode",
  "version": "0.0.1",
  "description": "Adds a dark mode to LinkedIn on web",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "./index.html",
    "default_title": "LinkedIn Dark Mode",
    "default_icon":"./icon16.png"
  },
  "background" :{
    "scripts": ["third-party/jquery-3.5.1.js", "background.js"]
  },
  "content_scripts": [
      {
          "js": ["third-party/jquery-3.5.1.js", "content.js"],
          "matches": ["https://*.linkedin.com/*"]
      }
  ],
  "permissions": [
  		"tabs",
      "storage",
      "https://*.linkedin.com/*",
      "activeTab"
  ]
}