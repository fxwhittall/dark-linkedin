{
  "name": "LinkedIn Dark Mode",
  "version": "0.0.1",
  "description": "Adds a dark mode to LinkedIn on web",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "./index.html",
    "default_title": "LinkedIn Dark Mode",
    "default_icon":"./icon16.png"
  },
  "content_scripts": [
      {
          "css": ["content.css"],
          "matches": ["https://*.linkedin.com/feed/*", "https://*.linkedin.com/notifications/*", "https://*.linkedin.com/mynetwork/*", "https://*.linkedin.com/jobs/*", "https://*.linkedin.com/messaging/*", "https://*.linkedin.com/in/*", "https://*.linkedin.com/company/*", "https://*.linkedin.com/school/*", "https://*.linkedin.com/search/*", "https://*.linkedin.com/psettings/*", "https://*.linkedin.com/public-profile/*", "https://*.linkedin.com/groups/*"]
      }
  ],
  "permissions": [
  		"tabs",
      "storage",
      "https://*.linkedin.com/*"
  ]
}